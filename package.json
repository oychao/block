{
  "name": "react-router-alive",
  "version": "1.0.0-beta.6",
  "main": "./bin/bundle.js",
  "author": {
    "name": "ouyang-chao",
    "email": "chao@ouyang.io"
  },
  "license": "WTFPL",
  "repository": "https://github.com/oychao/react-router-alive",
  "keywords": [
    "react",
    "keep-alive"
  ],
  "description": "A keep-alive route component for react-router",
  "files": [
    "LICENSE.md",
    "README.md",
    "bin/"
  ],
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint .",
    "fix": "eslint . --fix --ext .js",
    "test": "cross-env NODE_ENV=test mocha --compilers @babel/register test/**/*.spec.js",
    "prepare": "npm run build",
    "watch": "npm-watch build"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-do-expressions": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-bind": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "^8.2.6",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "mocha-sinon": "^2.1.0",
    "npm-watch": "^0.6.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.3",
    "sinon": "^6.1.5"
  },
  "dependencies": {
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-is": "^16.8.5",
    "react-router": "^5.0.0",
    "tiny-invariant": "^1.0.3",
    "tiny-warning": "^1.0.2"
  }
